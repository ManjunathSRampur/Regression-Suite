<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="workflow">
	<groups>
		<run>
			<include name="Workflow" />
		</run>
	</groups>
	<listeners>
		<listener class-name="com.qa.ExtentReportListener.ExtentReportListener" />
	</listeners>
	<test name="BPS Workflow" preserve-order="true">
		<classes>

			<class name="com.qa.bps.test.FileUploadTest" />
			<class name="com.qa.bps.test.PlannerTest" />
			<class name="com.qa.bps.test.PlanDetailsTest" />
			<class name="com.qa.bps.test.ManagerViewTest" />
			<class name="com.qa.bps.test.ProductOverviewTest" />
			<class name="com.qa.bps.test.ProductViewDelayMgmtTest" />
		  <!--<class name="com.qa.bps.test.ShiftSummaryHandoverTest" /> -->  
		</classes>
	</test>
</suite> <!-- Suite -->